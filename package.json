{
  "name": "scdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .next .turbo node_modules/.cache",
    "compile": "tsc",
    "dev": "next dev --turbo",
    "lint:next": "next lint",
    "lint:more": "npx eslint .",
    "prettier:write": "npx prettier --write .",
    "build:release": "next build",
    "run:release": "next start",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:backup": "npx dotenv -e .env.local -- node database/backup.mjs",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm install",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.11.1",
    "@aws-sdk/client-s3": "3.937.0",
    "@aws-sdk/s3-presigned-post": "3.937.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/roboto": "5.2.8",
    "@hello-pangea/dnd": "18.0.1",
    "@mui/icons-material": "7.3.5",
    "@mui/material": "7.3.5",
    "@mui/x-data-grid": "8.19.0",
    "@mui/x-date-pickers": "8.19.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "7.0.0",
    "dotenv": "^17.2.3",
    "googleapis": "166.0.0",
    "luxon": "3.7.2",
    "next": "16.0.3",
    "next-auth": "4.24.13",
    "react": "19.2.0",
    "react-calendar-heatmap": "1.10.0",
    "react-dom": "19.2.0",
    "react-markdown": "10.1.0",
    "react-use": "17.6.0",
    "sharp": "0.34.5",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@next/bundle-analyzer": "16.0.3",
    "@types/luxon": "3.7.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.6",
    "@types/react-calendar-heatmap": "1.9.0",
    "@types/react-dom": "19.2.3",
    "dotenv-cli": "11.0.0",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.3",
    "prettier": "3.6.2",
    "prisma": "7.0.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0"
  }
}
