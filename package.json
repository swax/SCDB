{
  "name": "scdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "dev": "next dev --turbo",
    "lint:next": "next lint",
    "lint:more": "npx eslint .",
    "prettier:write": "npx prettier --write .",
    "build:release": "next build",
    "run:release": "next start",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:backup": "npx dotenv -e .env.local -- node database/backup.mjs",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm install",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.7.2",
    "@aws-sdk/client-s3": "3.675.0",
    "@aws-sdk/s3-presigned-post": "3.675.0",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@fontsource/roboto": "5.1.0",
    "@hello-pangea/dnd": "17.0.0",
    "@mui/icons-material": "6.1.4",
    "@mui/material": "6.1.4",
    "@mui/x-data-grid": "7.21.0",
    "@mui/x-date-pickers": "7.21.0",
    "@prisma/client": "5.21.1",
    "googleapis": "144.0.0",
    "luxon": "3.5.0",
    "next": "14.2.15",
    "next-auth": "4.24.8",
    "react": "18.3.1",
    "react-calendar-heatmap": "1.9.0",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.1",
    "react-use": "17.5.1",
    "sharp": "0.33.5",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@next/bundle-analyzer": "14.2.15",
    "@types/luxon": "3.4.2",
    "@types/node": "22.7.7",
    "@types/react": "18.3.11",
    "@types/react-calendar-heatmap": "1.9.0",
    "@types/react-dom": "18.3.1",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.15",
    "prettier": "3.3.3",
    "prisma": "5.21.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.10.0"
  }
}
