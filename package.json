{
  "name": "scdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "run:dev": "next dev",
    "lint:next": "next lint",
    "prettier:write": "npx prettier --write .",
    "build:release": "next build",
    "run:release": "next start",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "1.0.5",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.8",
    "@mui/icons-material": "5.14.15",
    "@mui/material": "5.14.15",
    "@mui/x-date-pickers": "^6.17.0",
    "@prisma/client": "5.5.2",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "luxon": "^3.4.3",
    "next": "14.0.1",
    "next-auth": "4.24.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "slugify": "1.6.6",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.3",
    "dotenv-cli": "7.3.0",
    "prettier": "3.0.3",
    "prisma": "5.5.2"
  }
