{
  "name": "scdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "dev": "next dev --turbo",
    "lint:next": "next lint",
    "lint:more": "npx eslint .",
    "prettier:write": "npx prettier --write .",
    "build:release": "next build",
    "run:release": "next start",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:backup": "npx dotenv -e .env.local -- node database/backup.mjs",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm install",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.2.0",
    "@aws-sdk/client-s3": "3.598.0",
    "@aws-sdk/s3-presigned-post": "3.598.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@fontsource/roboto": "5.0.13",
    "@hello-pangea/dnd": "16.6.0",
    "@mui/icons-material": "5.15.20",
    "@mui/material": "5.15.20",
    "@mui/x-data-grid": "7.7.0",
    "@mui/x-date-pickers": "7.7.0",
    "@next/bundle-analyzer": "14.2.4",
    "@prisma/client": "5.15.0",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint-config-next": "14.2.4",
    "luxon": "3.4.4",
    "next": "14.2.4",
    "next-auth": "4.24.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.1",
    "react-use": "17.5.0",
    "sharp": "0.33.4",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@types/luxon": "3.4.2",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "prettier": "3.3.2",
    "prisma": "5.15.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.13.0"
  }
}
