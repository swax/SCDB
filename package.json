{
  "name": "scdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .next .turbo node_modules/.cache",
    "compile": "tsc",
    "dev": "next dev --turbo",
    "lint": "eslint",
    "prettier:write": "npx prettier --write .",
    "build:release": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "run:release": "next start",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:backup": "npx dotenv -e .env.local -- node database/backup.mjs",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm install",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/s3-presigned-post": "^3.943.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/x-data-grid": "^8.21.0",
    "@mui/x-date-pickers": "^8.21.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "dotenv": "^17.2.3",
    "googleapis": "^167.0.0",
    "luxon": "^3.7.2",
    "next": "^16.0.7",
    "next-auth": "^4.24.13",
    "react": "^19.2.1",
    "react-calendar-heatmap": "^1.10.0",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "react-use": "^17.6.0",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@next/bundle-analyzer": "^16.0.7",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-calendar-heatmap": "^1.9.0",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
