{
  "name": "scdb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "dev": "next dev --turbo",
    "lint:next": "next lint",
    "lint:more": "npx eslint .",
    "prettier:write": "npx prettier --write .",
    "build:release": "next build",
    "run:release": "next start",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:backup": "npx dotenv -e .env.local -- node database/backup.mjs",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm install",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.11.0",
    "@aws-sdk/client-s3": "3.913.0",
    "@aws-sdk/s3-presigned-post": "3.913.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/roboto": "5.2.8",
    "@hello-pangea/dnd": "18.0.1",
    "@mui/icons-material": "7.3.4",
    "@mui/material": "7.3.4",
    "@mui/x-data-grid": "8.14.1",
    "@mui/x-date-pickers": "8.14.1",
    "@prisma/client": "6.17.1",
    "googleapis": "164.1.0",
    "luxon": "3.7.2",
    "next": "15.5.6",
    "next-auth": "4.24.11",
    "react": "19.2.0",
    "react-calendar-heatmap": "1.10.0",
    "react-dom": "19.2.0",
    "react-markdown": "10.1.0",
    "react-use": "17.6.0",
    "sharp": "0.34.4",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@next/bundle-analyzer": "15.5.6",
    "@types/luxon": "3.7.1",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-calendar-heatmap": "1.9.0",
    "@types/react-dom": "19.2.2",
    "dotenv-cli": "10.0.0",
    "eslint": "9.38.0",
    "eslint-config-next": "15.5.6",
    "prettier": "3.6.2",
    "prisma": "6.17.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  }
}
